		<Annunciation>
		  <Type>Memo</Type>
		  <Text>AUTOBRAKES</Text>
		  <Condition Suffix=" RTO">
		    <Equal>
			  <Simvar name="AUTO BRAKE SWITCH CB" unit="Enum"/>
