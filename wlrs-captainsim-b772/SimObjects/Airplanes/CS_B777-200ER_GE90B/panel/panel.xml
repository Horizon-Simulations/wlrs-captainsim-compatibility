					<Equal>
			  			<Simvar name="ENG ANTI ICE:1" unit="Boolean"/>
			  			<Constant>1</Constant>
					</Equal>
					<Equal>
			  			<Simvar name="ENG ANTI ICE:2" unit="Boolean"/>
			  			<Constant>1</Constant>
					</Equal>
					<Equal>
			  			<Simvar name="STRUCTURAL DEICE SWITCH" unit="Boolean"/>
