		<Annunciation>
			<Type>Warning</Type>
			<Text>FIRE APU</Text>
			<Condition>
				<Simvar name="APU ON FIRE DETECTED" unit="Bool"/>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>CONFIG RUDDER</Text>
			<Condition>
			<And>
					<Simvar name="SIM ON GROUND" unit="Bool"/>
				<Or>
					<Lower>
						<Simvar name="RUDDER TRIM" unit="Radians"/>
						<Constant>0</Constant>
					</Lower>
					<Greater>
						<Simvar name="RUDDER TRIM" unit="Radians"/>
						<Constant>0</Constant>
					</Greater>
				</Or>
				<Or>
					<GreaterEqual>
						<Simvar name="TURB ENG N1:1" unit="Percent"/>
						<Constant>75</Constant>
					</GreaterEqual>
					<GreaterEqual>
						<Simvar name="TURB ENG N1:2" unit="Percent"/>
						<Constant>75</Constant>
					</GreaterEqual>
				</Or>
			</And>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>CONFIG FLAPS</Text>
			<Condition>
			<And>
					<Simvar name="SIM ON GROUND" unit="Bool"/>
