	<!-- Annunciations -->

	<Annunciations>
		<Annunciation>
			<Type>Warning</Type>
			<Text>AUTOPILOT DISC</Text>
			<Condition>
				<StateMachine>
					<State id="Off" value="0">
						<Transition to="On">
							<Equal>
								<Simvar name="AUTOPILOT MASTER" unit="Boolean"/>
								<Constant>1</Constant>
							</Equal>
						</Transition>
					</State>
					<State id="On" value="0">
						<Transition to="Alerting">
							<Equal>
								<Simvar name="AUTOPILOT MASTER" unit="Boolean"/>
								<Constant>0</Constant>
							</Equal>
						</Transition>
					</State>
					<State id="Alerting" value="1">
						<Transition to="On">
							<Equal>
								<Simvar name="AUTOPILOT MASTER" unit="Boolean"/>
								<Constant>1</Constant>
							</Equal>
						</Transition>
					</State>
				</StateMachine>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>AUTOTHROTTLE DISC</Text>
			<Condition>
				<StateMachine>
					<State id="Off" value="0">
						<Transition to="On">
							<Equal>
								<Simvar name="AUTOPILOT THROTTLE ARM" unit="Boolean"/>
								<Constant>1</Constant>
							</Equal>
						</Transition>
					</State>
					<State id="On" value="0">
						<Transition to="Alerting">
							<Equal>
								<Simvar name="AUTOPILOT THROTTLE ARM" unit="Boolean"/>
								<Constant>0</Constant>
							</Equal>
						</Transition>
					</State>
					<State id="Alerting" value="1">
						<Transition to="On">
							<Equal>
								<Simvar name="AUTOPILOT THROTTLE ARM" unit="Boolean"/>
								<Constant>1</Constant>
							</Equal>
						</Transition>
					</State>
				</StateMachine>
			</Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>AIRSPEED LOW</Text>
		  <Condition>
		  	<And>
				<LowerEqual>
			  		<Simvar name="AIRSPEED INDICATED" unit="knots"/>
			  		<Simvar name="L:74S_FMC_MIN_MANUEVER_SPEED" unit="knots"/>		  
				</LowerEqual>
				<GreaterEqual>
					<Simvar name="PLANE ALT ABOVE GROUND MINUS CG" unit="feet"/>
					<Constant>55</Constant>
			  	</GreaterEqual>
			</And>
		  </Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>OVERSPEED</Text>
			<Condition>
				<Equal>
				<Simvar name="OVERSPEED WARNING" unit="Bool"/>
				<Constant>1</Constant>
				</Equal>
			</Condition>
		</Annunciation>
		
		<Annunciation>
			<Type>Warning</Type>
			<Text>FIRE ENG L</Text>
			<Condition>
				<Simvar name="ENG ON FIRE:1" unit="Bool"/>
