		<Annunciation>
		  <Type>Advisory</Type>
		  <Text>BLEED OFF ENG L</Text>
		  <Condition>
			<And>
				<Equal>
					<Simvar name="BLEED AIR ENGINE:1" unit="Boolean"/>
