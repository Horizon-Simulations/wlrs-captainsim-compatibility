			<Equal>
				<Simvar name="BLEED AIR APU" unit="Boolean"/>
				<Constant>0</Constant>
			</Equal>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>ICING WING</Text>
		  <Condition>
			<And>
				<GreaterEqual>
					<Simvar name="STRUCTURAL ICE PCT" unit="percent"/>
					<Constant>5</Constant>
				</GreaterEqual>
				<Equal>
			  		<Simvar name="STRUCTURAL DEICE SWITCH" unit="Boolean"/>
			  		<Constant>0</Constant>
				</Equal>
			</And>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>ICING ENG</Text>
		  <Condition>
			<And>
					<GreaterEqual>
						<Simvar name="STRUCTURAL ICE PCT" unit="percent"/>
						<Constant>5</Constant>
					</GreaterEqual>
				<Or>
					<Equal>
						<Simvar name="ENG ANTI ICE:1" unit="Boolean"/>
