					<Simvar name="TURB ENG N2:2" unit="percent"/>
					<Constant>60</Constant>
				</GreaterEqual>
			</And>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Advisory</Type>
		  <Text>BLEED OFF APU</Text>
