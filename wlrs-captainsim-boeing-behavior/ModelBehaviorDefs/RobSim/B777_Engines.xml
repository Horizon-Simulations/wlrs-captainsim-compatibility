<ModelBehaviors>
    <Template Name="HorizonSim_CS773_ENGINE_Turbine_Template">
        <DefaultTemplateParameters>
            <ENG_ID>1</ENG_ID>
            <MIN_RPM_FOR_SLOW>8000</MIN_RPM_FOR_SLOW>
            <MIN_RPM_FOR_BLUR>19000</MIN_RPM_FOR_BLUR>
        </DefaultTemplateParameters>
        <Component ID="#ANIM_NODE#">
            <UseTemplate Name="ASOBO_GT_Anim">
                <ANIM_NAME>#ANIM_NODE#</ANIM_NAME>
                <ANIM_CODE>

                    (A:GENERAL ENG RPM:#ENG_ID#, rpm) 5 &gt;
                    if{
                    0.01 3 2602 (A:TURB ENG N1:#ENG_ID#, Percent) (A:ANIMATION DELTA TIME, seconds) * * * *
                    } els{
                    (A:AIRCRAFT WIND Z, knots) 7 / neg
                    }

                </ANIM_CODE>
                <ANIM_LENGTH>360</ANIM_LENGTH>
                <ANIM_WRAP>1</ANIM_WRAP>
                <ANIM_DELTA>1</ANIM_DELTA>
            </UseTemplate>
        </Component>
        <Component ID="ENGINE_Turbine_#ENG_ID#_STILL" Node="PROP_#ENG_ID#_STILL_LEFT">
            <UseTemplate Name="ASOBO_GT_Visibility">
                <VISIBILITY_CODE>(A:GENERAL ENG RPM:#ENG_ID#, rpm) #MIN_RPM_FOR_SLOW# &lt;</VISIBILITY_CODE>
            </UseTemplate>
        </Component>

        <Component ID="ENGINE_Turbine_#ENG_ID#_SLOW" Node="PROP_#ENG_ID#_SLOW_LEFT">
            <UseTemplate Name="ASOBO_GT_Visibility">
                <VISIBILITY_CODE>
                    (A:GENERAL ENG RPM:#ENG_ID#, rpm) #MIN_RPM_FOR_SLOW# &gt;
                    (A:GENERAL ENG RPM:#ENG_ID#, rpm) #MIN_RPM_FOR_BLUR# &lt; and
                </VISIBILITY_CODE>
            </UseTemplate>
        </Component>
        <Component ID="ENGINE_Turbine_#ENG_ID#_BLUR" Node="PROP_#ENG_ID#_BLURRED_LEFT">
            <UseTemplate Name="ASOBO_GT_Visibility">
                <VISIBILITY_CODE>(A:GENERAL ENG RPM:#ENG_ID#, rpm) #MIN_RPM_FOR_BLUR# &gt;</VISIBILITY_CODE>
            </UseTemplate>
        </Component>
    </Template>
</ModelBehaviors>
